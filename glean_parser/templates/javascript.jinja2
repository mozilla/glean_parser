    {% for arg_name in (obj.type|args).common if obj[arg_name] is defined %}
    {{ arg_name|camelize }}: {{ obj[arg_name]|js }},
    {% endfor %}
}, {{ obj.type|class_name }}, {% if obj.labels is not none %}{{ obj.labels|js }}{% endif %}){% endmacro %}
{% if lang == "javascript" %}
"use strict";

{% endif %}
